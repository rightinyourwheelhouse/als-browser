{
  "name": "brainweb",
  "private": true,
  "version": "1.0.0",
  "description": "Browser for the ALS-liga",
  "main": "app/index.js",
  "author": "rightinyourwheelhouse",
  "repository": {
    "type": "git",
    "url": "https://github.com/rightinyourwheelhouse/als-browser.git"
  },
  "license": "MIT",
  "build": {
    "productName": "Brainweb",
    "appId": "com.brainweb.app",
    "icon": "icon.png",
    "win": {
      "icon": "build/icon.png"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "rightinyourwheelhouse",
        "repo": "als-browser"
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:start": "electron .",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on -c waitOnConfig.json http-get://localhost:3000/ && electron .\"",
    "electron:build-mac": "npm run build && electron-builder --mac",
    "electron:build-win": "npm run build && electron-builder --win",
    "electron:build:all": "npm run build && electron-builder -mw",
    "electron:deploy": "npm run build && electron-builder build --win --publish always"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@firebase/auth": "^0.19.12",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@mui/x-data-grid": "^5.11.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "firebase": "^9.6.11",
    "formik": "^2.2.9",
    "fuse.js": "^6.6.2",
    "node-fpgrowth": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-electron-browser-window": "^0.1.2",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "concurrently": "^6.2.0",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.28.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.23",
    "vite": "^2.9.0",
    "wait-on": "^5.3.0"
  }
}
